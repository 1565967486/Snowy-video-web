<view><scroll-view class="{{['DrawerPage',(isEdit)?'show':'']}}" scroll-y><view class="header"><view class="header-mask"></view><image class="header-background" src="{{backgroundImage}}"></image><view data-event-opts="{{[['tap',[['handleEdit',['$event']]]]]}}" class="cu-avatar round user-avatar" bindtap="__e"><image class="round" style="width:100%;height:100%;" src="{{avatarUrl}}"></image><view class="{{['cu-tag badge',gender.class]}}"></view></view><view data-event-opts="{{[['tap',[['handleEdit',['$event']]]]]}}" class="cu-btn round nickname text-white" bindtap="__e">{{''+nickname+''}}</view><view class="user-info"><block wx:for="{{userInfo}}" wx:for-item="item" wx:for-index="__i0__" wx:key="*this"><block><view data-event-opts="{{[['tap',[['toFriendList',['$event']]]]]}}" class="cu-capsule radius" bindtap="__e"><view class="{{['cu-tag tag-title',item.color]}}">{{''+item.title+''}}</view><view class="cu-tag line-white">{{item.value}}</view></view></block></block></view></view><scroll-view class="bg-white nav" scroll-x><view class="flex text-center"><block wx:for="{{tabList}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view class="{{['cu-item flex-sub',index==tabCur?'text-blue current':'']}}" data-id="{{index}}" data-event-opts="{{[['tap',[['tabSelect',['$event']]]]]}}" bindtap="__e">{{''+item+''}}</view></block></view></scroll-view><block wx:if="{{tabCur===0}}"><contribute-list style="margin-bottom:140rpx;" vue-id="1" userId="{{userId}}" bind:__l="__l"></contribute-list></block><block wx:else><like-list style="margin-bottom:140rpx;" vue-id="2" userId="{{userId}}" bind:__l="__l"></like-list></block><view style="height:120rpx;"></view></scroll-view><view data-event-opts="{{[['tap',[['handleEdit',['$event']]]]]}}" class="{{['DrawerClose',(isAvatarEdit)?'show':'']}}" bindtap="__e"><text class="cuIcon-pullright"></text></view><drawer-left vue-id="3" isAvatarEdit="{{isAvatarEdit}}" userData="{{userData}}" isMe="{{true}}" data-event-opts="{{[['^handleEdit',[['handleEdit']]],['^changeGender',[['changeGender']]],['^changeNickName',[['changeNickName']]],['^changeAvatar',[['changeAvatar']]],['^changeBackgroundImage',[['changeBackgroundImage']]]]}}" bind:handleEdit="__e" bind:changeGender="__e" bind:changeNickName="__e" bind:changeAvatar="__e" bind:changeBackgroundImage="__e" bind:__l="__l"></drawer-left></view>