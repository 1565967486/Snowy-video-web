<view><scroll-view class="{{['DrawerPage',(isEdit)?'show':'']}}" scroll-y><view class="header"><view class="header-mask"></view><text data-event-opts="{{[['tap',[['handleBack',['$event']]]]]}}" class="back-icon cuIcon-back" bindtap="__e"></text><image class="header-background" src="{{backgroundImage}}"></image><view data-event-opts="{{[['tap',[['handleEdit',['$event']]]]]}}" class="cu-avatar round user-avatar" bindtap="__e"><image class="round" style="width:100%;height:100%;" src="{{avatarUrl}}"></image><view class="{{['cu-tag badge',gender.class]}}"></view></view><button data-event-opts="{{[['tap',[['handleEdit',['$event']]]]]}}" class="cu-btn round nickname text-white" bindtap="__e">{{''+nickname+''}}</button><block wx:if="{{!isMe}}"><button data-event-opts="{{[['tap',[['handleFollow',['$event']]]]]}}" class="{{['cu-btn round attentionBtn text-white',(isFollow)?'bg-gradual-green':'']}}" bindtap="__e"><text class="{{['margin-bottom-sm',isFollow?'cuIcon-likefill':'cuIcon-like']}}"></text>{{''+(isFollow?'已关注':'关 注')+''}}</button></block><view class="user-info"><block wx:for="{{userInfo}}" wx:for-item="item" wx:for-index="__i0__" wx:key="*this"><block><view data-event-opts="{{[['tap',[['toFriendList',['$event']]]]]}}" class="cu-capsule radius" bindtap="__e"><view class="{{['cu-tag tag-title',item.color]}}">{{''+item.title+''}}</view><view class="cu-tag line-white">{{item.value}}</view></view></block></block></view></view><scroll-view class="bg-white nav" scroll-x><view class="flex text-center"><block wx:for="{{tabList}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view class="{{['cu-item flex-sub',index==tabCur?'text-blue current':'']}}" data-id="{{index}}" data-event-opts="{{[['tap',[['tabSelect',['$event']]]]]}}" bindtap="__e">{{''+item+''}}</view></block></view></scroll-view><block wx:if="{{tabCur===0}}"><contribute-list style="margin-bottom:140rpx;" vue-id="1" userId="{{createrId}}" bind:__l="__l"></contribute-list></block><block wx:else><like-list style="margin-bottom:140rpx;" vue-id="2" userId="{{createrId}}" bind:__l="__l"></like-list></block></scroll-view><view data-event-opts="{{[['tap',[['handleEdit',['$event']]]]]}}" class="{{['DrawerClose',(isAvatarEdit)?'show':'']}}" bindtap="__e"><text class="cuIcon-pullright"></text></view><drawer-left vue-id="3" isAvatarEdit="{{isAvatarEdit}}" userData="{{userData}}" isMe="{{false}}" data-event-opts="{{[['^handleEdit',[['handleEdit']]]]}}" bind:handleEdit="__e" bind:__l="__l"></drawer-left></view>